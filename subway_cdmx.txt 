/* SCHEMA*/
CREATE TABLE stops(
  stop_id    INTEGER PRIMARY KEY,
  stop_name  TEXT, 
  stop_zone INTEGER,
  FOREIGN KEY(stop_zone) REFERENCES zones(zone_id)
);

CREATE TABLE zones(
  zone_id     INTEGER PRIMARY KEY AUTOINCREMENT, 
  zone_name   TEXT
);

CREATE TABLE routes(
  route_id     INTEGER PRIMARY KEY AUTOINCREMENT, 
  route_name   TEXT 
);

CREATE TABLE connections(
    stop_id INTEGER,
    route_id INTEGER,
    last_stop INTEGER,
  	next_stop INTEGER,
    PRIMARY KEY(stop_id,route_id),
    FOREIGN KEY(stop_id) REFERENCES stops,
    FOREIGN KEY(route_id) REFERENCES routes
);

CREATE TABLE subways(
	subway_id INTEGER PRIMARY KEY AUTOINCREMENT,
	subway_route INTEGER,
	subway_direction INTEGER,
	subway_current_stop INTEGER,
	FOREIGN KEY(subway_route) REFERENCES routes
);

CREATE TABLE drivers(
	driver_id INTEGER PRIMARY KEY AUTOINCREMENT,
	driver_fname TEXT,
	driver_lname TEXT,
	driver_shift_start INTEGER,
	driver_shift_hours INTEGER,
	driver_sub INTEGER,
	FOREIGN KEY(driver_sub) REFERENCES subways(subway_id)
);

CREATE TABLE disruptions(
    dis_id INTEGER PRIMARY KEY AUTOINCREMENT,
    dis_name TEXT,
    dis_stop INTEGER,
    dis_route INTEGER,
    dis_start_hr INTEGER,
    dis_startD INTEGER,
    dis_startM INTEGER,
    dis_startY INTEGER,
    dis_end_hr INTEGER,
    dis_endD INTEGER,
    dis_endM INTEGER,
    dis_endY INTEGER,
    FOREIGN KEY(dis_stop) REFERENCES stops(stop_id)
    FOREIGN KEY(dis_route) REFERENCES routes(route_id)
);


